# Source my zsh config
if [[ -e $HOME/Desktop/robaMia/configFile/zsh/zsh-config ]]; then
  source $HOME/Desktop/robaMia/configFile/zsh/zsh-config
fi
# Use manjaro zsh prompt
if [[ -e $HOME/Desktop/robaMia/configFile/zsh/zsh-prompt ]]; then
  source $HOME/Desktop/robaMia/configFile/zsh/zsh-prompt 
fi
